    "agent:dev": "next dev -p 3030",
    "agent:dev:3030": "next dev -p 3030",
    "build": "next build",
    "agent:build": "next build",
    "start": "next start -p 3030",
    "agent:start": "next start -p 3030",
    "agent:start:3030": "next start -p 3030",
    "agent:builder:init": "node ./scripts/agent-builder/init.mjs",
    "agent:builder:validate": "node ./scripts/agent-builder/validate.mjs",
    "agent:builder:generate": "node ./scripts/agent-builder/generate.mjs",
