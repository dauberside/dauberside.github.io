  "scripts": {
    "prebuild": "pnpm -s agent:builder:validate && pnpm -s agent:builder:generate",
    "postbuild": "node ./scripts/postbuild.mjs",
    "dev": "next dev --hostname 0.0.0.0 --port 3000",
    "dev:kb": "pnpm kb:build && pnpm dev",
